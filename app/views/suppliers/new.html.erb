<h1>New Supplier</h1>
<%= form_with model: @supplier, local: true do |form| %>
  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= form.label :supplier_number %>
    <%= form.number_field :supplier_number, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= form.submit 'Create Supplier', class: 'btn btn-primary' %>
  </div>
<% end %>
